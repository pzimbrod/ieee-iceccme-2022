@article{hibinoControlCrystallographicTexture2021,
  title = {Control of {{Crystallographic Texture}} and {{Mechanical Properties}} of {{Hastelloy-X}} via {{Laser Powder Bed Fusion}}},
  author = {Hibino, Shinya and Todo, Tsubasa and Ishimoto, Takuya and Gokcekaya, Ozkan and Koizumi, Yuichiro and Igashira, Kenichiroh and Nakano, Takayoshi},
  year = {2021},
