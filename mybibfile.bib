@article{jenaLaserPowderBed2021,
  title = {Laser {{Powder Bed Fusion}} of a {{New High}} Gamma Prime {{Ni-based Superalloy}} with {{Improved Weldability}}},
  author = {Jena, Ashutosh and Ece Atabay, Sila and Gontcharov, Alexandre B. and Lowden, Paul and Brochu, Mathieu},
  year = {2021},
  month = jun,
  journal = {Materials \& Design},
  pages = {109895},
  issn = {0264-1275},
  doi = {10.1016/j.matdes.2021.109895},
  abstract = {This study reports the crack-free fabrication of a new high gamma prime precipitation hardened Ni-based superalloy by laser powder bed fusion (LPBF) process. In the as-built condition, scanning electron microscopy and electron backscattered diffraction analysis indicated the presence of elongated columnar grains with an orientation along {$<$}100{$>$} direction. Following the sub-solvus heat treatment cycle determined using Thermo-Calc software, crack-free microstructure with columnar grains was observed along {$<$}100{$>$} orientation. High-resolution scanning electron microscope analysis revealed a bimodal distribution of gamma prime precipitates and a uniform distribution of carbide precipitates in the heat-treated samples. As a result of heat treatment, hardness increased from 365 HV to 508 HV. The heat-treated samples also displayed excellent yield strength, ultimate tensile strength, and elongation of 939 MPa, 1368 MPa, and 23 \% respectively at room temperature. Mechanical testing at elevated temperatures confirmed the yield strength, tensile strength, and elongation reached 80 MPa, 106 MPa, and 34 \% respectively at 1121 \textdegree C. The stress rupture test of the heat-treated samples revealed good creep properties with rupture times of 559 hours and 75 hours, respectively, at 927 \textdegree C and 982 \textdegree C, for stresses of 152 MPa and 138 MPa.},
  langid = {english},
  keywords = {Additive manufacturing,Bimodal distribution,Laser Powder Bed Fusion (LPBF),Mechanical property,Ni-based superalloy},
  file = {/Users/zimbropa/Documents/Literaturverwaltung/storage/UXME3KSR/S0264127521004482.html}
}

@article{jeongArchitecturedHeterogeneousAlloys2022,
  title = {Architectured Heterogeneous Alloys with Selective Laser Melting},
  author = {Jeong, Sang Guk and Karthik, Gangaraju Manogna and Kim, Eun Seong and Zargaran, Alireza and Ahn, Soung Yeoul and Sagong, Man Jae and Kang, Suk Hoon and Cho, Jung-Wook and Kim, Hyoung Seop},
  year = {2022},
  month = feb,
  journal = {Scripta Materialia},
  volume = {208},
  pages = {114332},
  issn = {1359-6462},
  doi = {10.1016/j.scriptamat.2021.114332},
  abstract = {Hetero-structuring is considered as one of the effective strengthening routes in metallic materials to realize excellent strength-ductility synergy. However, tailoring several microstructural features in these heterogeneous materials is challenging. In this work, we demonstrate a new capability of selective laser melting in controlling the site-specific microstructures and developing architectured heterogeneous materials. The heterostructured stainless steel samples demonstrated an excellent combination of strength and ductility compared to homogeneous materials. The current approach is versatile and can be used for many printable alloys. Further, this work opens a new direction in metal additive manufacturing to produce heterostructured materials for enhanced strength and ductility.},
  langid = {english},
  keywords = {Architectured materials,Austenitic stainless steels,Heterostructured materials,Laser powder-bed fusion,Metal additive manufacturing},
  file = {/Users/zimbropa/Documents/Literaturverwaltung/storage/F9PAJ7FK/S1359646221006126.html}
}

@article{jiangAlCoCrFeNiHighEntropy2021,
  title = {{{AlCoCrFeNi}} High Entropy Alloy Fabricated via Selective Laser Melting Reinforced by {{Fe-based}} Metallic Glass},
  author = {Jiang, Qi and Zhang, Peilei and Yu, Zhishui and Tian, Yingtao and Ma, Songyun},
  year = {2021},
  month = oct,
  journal = {Materials Letters},
  pages = {130994},
  issn = {0167-577X},
  doi = {10.1016/j.matlet.2021.130994},
  abstract = {The 5\% Fe-based amorphous reinforced AlCoCrFeNi high-entropy alloy (HEA) specimens were prepared by selective laser melting (SLM) technique. The mixed of Fe-based amorphous reduces the grain diameter and eliminates the presence of texture. Meanwhile, the anisotropy of the specimen was reduced. The addition of Fe-based amorphous causes the precipitation of FCC phase in the body-centered cubic (BCC) matrix, and the face-centered cubic (FCC) phase is uniformly distributed at the grain boundaries. The presence of FCC phase significantly reduces the internal stress of the specimen. The elements in the amorphous alloy solidly dissolve into the BCC matrix during the printing process, further strengthening the BCC matrix. The residual amorphous and nanocrystalline phases also result in a significant improvement in the performance of the specimen.},
  langid = {english},
  keywords = {3D printing,Amorphous materials,Composite materials,High-entropy alloys,Microstructure},
