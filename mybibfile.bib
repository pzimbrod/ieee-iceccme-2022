@article{valenteInsituInterstitialAlloying2021,
  title = {In-Situ Interstitial Alloying during Laser Powder Bed Fusion of {{AISI}} 316 for Superior Corrosion Resistance},
  author = {Valente, Emilie H{\o}rdum and Nadimpalli, Venkata Karthik and Christiansen, Thomas L. and Pedersen, David Bue and Somers, Marcel A. J.},
