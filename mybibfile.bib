@article{boillatEffectNanostructuresAluminum2021,
  title = {The {{Effect}} of {{Nanostructures}} in {{Aluminum Alloys Processed Using Additive Manufacturing}} on {{Microstructural Evolution}} and {{Mechanical Performance Behavior}}},
