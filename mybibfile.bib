}

@article{liuStrengtheningMechanismMicropillar2020,
  title = {Strengthening Mechanism and Micropillar Analysis of High-Strength {{NiTi}}\textendash{{Nb}} Eutectic-Type Alloy Prepared by Laser Powder Bed Fusion},
  author = {Liu, Shifeng and Han, Song and Zhang, Ling and Chen, Liang-Yu and Wang, Liqiang and Zhang, Liang and Tang, Yujin and Liu, Jia and Tang, Huiping and Zhang, Lai-Chang},
  year = {2020},
  month = jan,
  journal = {Composites Part B: Engineering},
  pages = {108358},
  issn = {1359-8368},
  doi = {10.1016/j.compositesb.2020.108358},
  abstract = {Although laser powder bed fusion (LPBF) can produce complex structures with promising properties, there still exists a concern on the diversity in materials, especially composites, for LPBF. This work innovatively used NiTi and Nb powders to prepare NiTi\textendash Nb eutectic-type alloy by LPBF, which provides a new method to prepare NiTi\textendash Nb shape memory alloy and could be an alternative for tailoring microstructure. The semi-molten Nb particle phase was retained during LPBF, so that {$\beta$}-Nb and the eutectic structure display a gradient distribution in the Nb diffusion concentration. The heat treatment process (annealed at 850\,\textdegree C for 0.5\,h) makes NiTi\textendash Nb samples have high yield strength ({$\sim$}1640\,MPa), high compressive strength ({$\sim$}2380\,MPa), and high compressive strain ({$\sim$}39\%), which are superior to the corresponding ones of its as-built and as-cast counterparts. Large Nb phase particles with a size of 10\textendash 30\,{$\mu$}m are retained and accelerated the formation of eutectic phase and {$\beta$}-Nb precipitate phase. Overall, the presence of {$\beta$}-Nb phase has a beneficial effect on the alloy, and a large number of intertwined dislocations and stacking faults around the {$\beta$}-Nb phase promote the formation of martensite under stress loading. The eutectic phase makes an important contribution to the high strength and plasticity of the NiTi\textendash Nb alloy.},
  keywords = {Eutectic phase,Heat treatment,laser powder bed fusion,Micropillar compression,Strengthening mechanism}
}

