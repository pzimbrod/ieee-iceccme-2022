@article{hafeezEvolutionMicrostructuralComplex2021,
  title = {Evolution of Microstructural Complex Transitions in Low-Modulus {$\beta$}-Type {{Ti-35Nb-2Ta-3Zr}} Alloy Manufactured by Laser Powder Bed Fusion},
  author = {Hafeez, Noman and Wei, Daixiu and Xie, Lechun and Tang, Yujin and Liu, Jia and Kato, Hidemi and Lu, Weijie and Zhang, Lai-Chang and Wang, Liqiang},
  year = {2021},
  month = oct,
  journal = {Additive Manufacturing},
  pages = {102376},
  issn = {2214-8604},
  doi = {10.1016/j.addma.2021.102376},
  abstract = {Low modulus {$\beta$}-type Ti-35Nb-2Ta-3Zr alloy scaffold was fabricated by laser powder bed fusion (L-PBF). The complex microstructure and mechanical properties were characterized systematically. The martensitic interstitial complex transitions (ICTs) from {$\beta$} to {$\alpha$}" ({$\beta\rightarrow\alpha$}"), {$\alpha$}" to {$\omega$} ({$\alpha$}"-{$\omega$}), and {$\beta$} to {$\alpha$}" and {$\omega$} ({$\beta\rightarrow\alpha$}-{$\omega$}) phases are accompanied by dislocation pile-ups and twins in a heterogeneous manner. A homaloidal transition was observed with astraddle {$\omega$} and {$\alpha$}"M nanolayers and partial nanolayers along the boundaries of the {$\alpha$}"T martensitic twin. Crystallographic characterization confirmed that \{332\}{$\langle$}113{$\rangle$} and \{112\}{$\langle$}111{$\rangle$} twinning and shear stress assisted the {$\alpha$}-{$\omega$} and {$\beta\rightarrow\alpha$}" transitions at the interface of the {$\beta$} region. Both the {$\alpha$}ꞌꞌ martensitic twin and {$\omega$} formation were observed adjacent to the \{332\}{$\langle$}113{$\rangle$} type twinning mode. The [332] twinning were instigated and nucleated through {$\omega$}-phase formation. The secondary [112] twins amid the primary [332] twinning were instigated and nucleated through {$\omega$}-phase formation. Multiple slip bands were identified on the surface of the micropillar after the microcompression testing. Moreover, high-density dislocations and dislocation pile-ups were found alongside the twins and grain boundaries. In addition, \{112\}{$\langle$}111{$\rangle$} twinning was identified amongst dislocation pile-ups. This work reveals a novel complex phase transformation that could play a significant role in applications such as biomedical implants.},
  langid = {english},
  keywords = {{332}〈113〉 twinning,Homaloidal complex microstructure,α" martensitic transformation,α─ω transition,ω nanolayers},
  file = {/Users/zimbropa/Documents/Literaturverwaltung/storage/JCIBSP53/S2214860421005303.html}
}

@article{hafeezSuperelasticResponseLowmodulus2020,
