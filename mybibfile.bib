@article{ngHighStabilityHigh2021,
  title = {High Stability and High Strength {$\beta$}-Titanium Alloys for Additive Manufacturing},
  author = {Ng, C. H. and Bermingham, M. J. and Kent, D. and Dargusch, M. S.},
  year = {2021},
  month = jun,
  journal = {Materials Science and Engineering: A},
  volume = {816},
  pages = {141326},
  issn = {0921-5093},
  doi = {10.1016/j.msea.2021.141326},
  abstract = {Metastable {$\beta$}-Ti alloys are attractive for additive manufacturing as they offer high strength and toughness, with the ability to readily tailor properties through control of the phase fraction and morphology of precipitate phases. However, during additive manufacturing processes thermal cycling associated with repetitive layer-by-layer deposition can potentially promote unintended {$\beta$}-phase decomposition and in-situ precipitation of other phases. This work investigates the stability of the {$\beta$}-phase during high heat input (1648~J/mm) Wire Arc Additive Manufacturing (WAAM) of Ti\textendash 3Al\textendash 8V\textendash 6Cr\textendash 4Mo\textendash 4Zr (Beta-C). Thermocouples and pyrometers to first used to measure the thermal environment during WAAM, and SEM, XRD and TEM characterisation techniques are used to investigate the microstructure of the as-built alloy. Despite observing large thermal excursions during WAAM well above the temperatures necessary to precipitate {$\alpha$}-phase, no evidence of {$\beta$}-phase decomposition was found during WAAM. A Time-Temperature-Transformation (TTT) diagram is used to show that the cooling rates are too fast and the cumulative thermal exposures are too short to decompose the {$\beta$}-phase during WAAM. Consequently, the alloy retains the bcc {$\beta$}-phase which gives the alloy moderate strength (tensile strength \textasciitilde 748~MPa) and good ductility (\textasciitilde 20\%) during WAAM.},
  langid = {english},
  keywords = {Electron microscopy,Hardness,Phase transformation,Stress/strain measurement,Titanium alloys},
  file = {/Users/zimbropa/Documents/Literaturverwaltung/storage/MRXIAW3T/S0921509321005955.html}
}

