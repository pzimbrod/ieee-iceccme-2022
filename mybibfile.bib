@techreport{pedrazziniInfluencePowderbedTemperature2020,
  title = {Influence of Powder-Bed Temperature on the Microstructure and Mechanical  Properties of {{Ti-6Al-4V}} Produced by Selective Laser Melting},
  author = {Pedrazzini, S. and Pek, M. E. and Ackerman, A. K. and Ali, H. and Ghadbeigi, H. and Mumtaz, K. and Dessolier, T. and Britton, T. B. and Bajaj, P. and J{\"a}gle, E. and Gault, B. and London, A. J. and {Galindo-Nava}, E.},
  year = {2020},
  month = jun,
  abstract = {Advanced characterisation techniques were used on LPBF Ti-6Al-4V samples produced on a heated base plate. When the substrate temperature is 100\{\textbackslash deg\}C the elongation is 6\textbackslash\%, which increases and peaks at 10\textbackslash\% at 570\{\textbackslash deg\}C, then sharply decreases to zero ductility at 770\{\textbackslash deg\}C. At 100\{\textbackslash deg\}C, a heavily strained and twinned microstructure, primarily composed of \{\textbackslash alpha\}+\{\textbackslash alpha\}', was observed and it was comparable to asbuilt microstructures obtained by conventional LPBF methods. At higher temperatures, twins are no longer present and instead nano-scale \{\textbackslash beta\} precipitates are observed within \{\textbackslash alpha\}' and \{\textbackslash alpha\}, as well as dislocation networks (570\{\textbackslash deg\}C) and tangles (770\{\textbackslash deg\}C). Solute segregation at crystal defects was observed in all pre-heating conditions. Al and V segregation at microtwins was observed in the 100\{\textbackslash deg\}C sample, reporting for the first time `selective' and mutually exclusive Al- and V-rich regions forming in adjacent twins. V segregation at dislocations was observed in the 570\{\textbackslash deg\}C and 770\{\textbackslash deg\}C samples, consistent with the higher preheating temperatures. High O contents were measured in all samples but with apparent opposing effects. At 100\{\textbackslash deg\}C and 570\{\textbackslash deg\}C was estimated to be below the critical threshold for O embrittlement and locally aids in maintaining a strength high by solid solution strengthening, whereas at 770\{\textbackslash deg\}C it was above the threshold, therefore failing in a brittle fashion. Based on these observations, the initial increase in ductility from 100\{\textbackslash deg\}C to 570\{\textbackslash deg\}C is attributed to a reduction in microtwins and the dislocation networks acting as `soft barriers' for slip within a coarser microstructure. The lack of ductility at 770\{\textbackslash deg\}C was attributed to local solute redistribution causing dislocation pinning and an increase of O content in this sample.},
  file = {/Users/zimbropa/Documents/Literaturverwaltung/storage/ATP6KCN8/Pedrazzini et al. - 2020 - Influence of powder-bed temperature on the microst.pdf}
}

@article{peguesExploringAdditiveManufacturing2020,
  title = {Exploring Additive Manufacturing as a High-Throughput Screening Tool for Multiphase High Entropy Alloys},
  author = {Pegues, Jonathan W. and Melia, Michael A. and Puckett, Raymond and Whetten, Shaun R. and Argibay, Nicolas and Kustas, Andrew B.},
  year = {2020},
  month = jan,
  journal = {Additive Manufacturing},
  pages = {101598},
  issn = {2214-8604},
  doi = {10.1016/j.addma.2020.101598},
  abstract = {High entropy alloys (HEA) exhibit exceptional and sometimes record-setting physical, thermal and mechanical properties. However, the vast composition space presents a significant practical challenge for optimization, particularly when relying on sequential use of conventional metallurgical fabrication and characterization methods. An efficient method for rapidly evaluating the properties of complex concentrated alloys is presented. Powder based directed energy deposition additive manufacturing was used to synthesize a broad range of compositions in a single metallurgical sample via in situ alloying. An investigation of the phase-property space was done by compositionally grading a transition metal-based HEA (CoCrFeMnNi) with several refractory metals (i.e., Ta, Nb, and Ti-6Al-4\,V). A high throughput microstructural-mechanical analysis method was also utilized to rapidly assess microstructure and hardness as a function of alloy composition. Compositions promoting solid solutions, multiple phases, and intermetallics were found to have a wide range of hardness and fabricability. These results are discussed in the context of alloy-specific microstructural characteristics and configurational entropy, along with relevant processing challenges.},
  keywords = {additive manufacturing,complex concentrated alloys,material discovery,microstructural characterization,Refractory metals}
}

@article{peguesSituSynchrotronXray2021,
  title = {In Situ Synchrotron {{X-ray}} Imaging and Mechanical Properties Characterization of Additively Manufactured High-Entropy Alloy Composites},
  author = {Pegues, Jonathan W. and Melia, Michael A. and Rodriguez, Mark A. and Babuska, Tomas F. and Gould, Benjamin and Argibay, Nicolas and Greco, Aaron and Kustas, Andrew B.},
  year = {2021},
  month = mar,
  journal = {Journal of Alloys and Compounds},
  pages = {159505},
  issn = {0925-8388},
  doi = {10.1016/j.jallcom.2021.159505},
  abstract = {Laser beam directed energy deposition has become an increasingly popular advanced manufacturing technique for materials discovery as a result of the in situ alloying capability. In this study, we leverage an additive manufacturing enabled high throughput materials discovery approach to explore the composition space of a graded Wx(CoCrFeMnNi)100-x sample spanning 0 {$\leq$} x {$\leq$} 21at.\%. In addition to microstructural and mechanical characterization, synchrotron high speed x-ray computer aided tomography was conducted on a W20(CoCrFeMnNi)80 composition to visualize melting dynamics, powder-laser interactions, and remelting effects of previously consolidated material. Results reveal the formation of the Fe7W6 intermetallic phase at W concentrations {$>$} 6at.\%, despite the high configurational entropy. Unincorporated W particles also occured at W concentrations {$>$} 10at.\% accompanied by a dissolution band of Fe7W6 at the W/matrix interface and hardness values greater than 400 HV. The primary strengthening mechanism is attributed to the reinforcement of the Fe7W6 and W phases as a metal matrix composite. The in situ high speed x-ray imaging during remelting showed that an additional laser pass did not promote further mixing of the Fe7W6 or W phases suggesting that, despite the dissolution of the W into the Fe7W6 phase being thermodynamically favored, it is kinetically limited by the thickness/diffusivity of the intermetallic phase, and the rapid solidification of the laser-based process.},
  langid = {english},
  keywords = {Additive Manufacturing,Functional Grading,High-entropy alloys,Intermetallics,Refractories,Synchrotron X-Ray},
  file = {/Users/zimbropa/Documents/Literaturverwaltung/storage/73K7EZKA/S0925838821009142.html}
}

