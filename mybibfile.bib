@techreport{tsubouchiEvaluationSpatterParticles2021,
  type = {Preprint},
  title = {Evaluation of Spatter Particles, Metal Vapour Jets, and Depressions Considering Influence of Laser Incident Angle on Melt Pool Behaviour},
