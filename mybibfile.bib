  doi = {10.1016/j.compositesb.2021.109555},
  abstract = {In-situ Y2O3 nanoparticles decorated Inconel 738 LC composite by powder mixture is fabricated using laser powder bed fusion with different scan speeds and laser powers. The process window is significantly enlarged with the addition of 0.05~wt\% Y2O3 by removing the cracks in the as-printed parts. Well-dispersed Y4Al2O9 particles are formed during the reaction between the mixed powders and the laser beam. Obvious enrichment of Zr in the Y4Al2O9 particles by substitution for the yttrium is observed, which effectively eliminates the segregation of Zr along the grain boundaries. In this case, the cracks are mitigated since the formation of liquid film is correspondingly hindered at the last stage of solidification. In addition, the gains are coarsened to nearly double the size with the incorporation of Y2O3 nanoparticles before and after heat treatment. As a result, the Y2O3-inoculated Inconel 738 LC exhibits an improved strength at 850~\textdegree C.},
  langid = {english},
  keywords = {Cracking,Laser powder bed fusion,Mechanical properties,Metal-matrix composites,Microstructure}
}

@article{gurtlerInfluencePowderDistribution2014,
  title = {Influence of Powder Distribution on Process Stability in Laser Beam Melting: {{Analysis}} of Melt Pool Dynamics by Numerical Simulations},
  author = {G{\"u}rtler, Franz-Josef and Karg, Michael and Dobler, Michael and Kohl, Stefanie and Tzivilsky, Ilya and Schmidt, Michael},
