@article{vargasMatrixfreeApproachesGPU2021,
  title = {Matrix-Free Approaches for {{GPU}} Acceleration of a High-Order Finite Element Hydrodynamics Application Using {{MFEM}}, {{Umpire}}, and {{RAJA}}},
