@article{vecchiatoMeltPoolMicrostructure2020,
  title = {Melt Pool Microstructure and Morphology from Single Exposures in Laser Powder Bed Fusion of {{316L}} Stainless Steel},
  author = {Vecchiato, F. L. and Winton, H. and Hooper, P. A. and Wenman, M. R.},
  year = {2020},
  month = jan,
  journal = {Additive Manufacturing},
  pages = {101401},
  issn = {2214-8604},
  doi = {10.1016/j.addma.2020.101401},
  abstract = {Single depositions of 316L stainless steel in pulsed laser powder bed fusion were performed to understand the influence of energy density input strategy on melt pool morphology, microstructure and temperature fields. Depositions performed at equal energy inputs, but variable laser powers, produced keyhole mode depositions for laser powers {$\geq$} 150W and conduction mode depositions for laser powers below 150W. The laser power combined to the laser exposure time was found to control the penetration depth, increasing from 50{$\mu$}m to 300{$\mu$}m as the laser exposure increases. An increase in laser exposure time was found to decrease the isotherm liquidus velocity, from 1.2mms-1 to 0.2mms-1. EBSD maps of deposition cross-sections highlighted the change in temperature gradients as the melt pool increased in depth during keyhole and conduction mode depositions. Strong sub-surface temperature gradients were found for keyhole mode with weak gradients above the surface. In conduction mode variation in the temperature gradient throughout the melt pool was less pronounced. The decrease in cooling rate as the exposure time is increased was highlighted in coarsening of the cell spacing produced by the solute redistribution during solidification, diverging between bottom and dome of the deposition melt pool.},
  keywords = {316L,Absorbed energy,additive manufacturing,Cellular structure,Cooling rate,EBSD,Isotherm velocity,laser powder bed fusion,LPBF,Melt pool,Microstructure}
}

