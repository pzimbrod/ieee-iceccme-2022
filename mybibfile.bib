@article{promoppatumInfluenceMaterialConstitutive2020,
  title = {Influence of Material Constitutive Models on Thermomechanical Behaviors in the Laser Powder Bed Fusion of {{Ti-6Al-4V}}},
  author = {Promoppatum, Patcharapit and Rollett, Anthony D.},
  year = {2020},
  month = jan,
  journal = {Additive Manufacturing},
  pages = {101680},
  issn = {2214-8604},
  doi = {10.1016/j.addma.2020.101680},
  abstract = {A transient three-dimensional thermomechanical model was developed to examine the evolution of thermal and mechanical fields in the laser powder bed fusion of Ti-6Al-4V alloy, particularly with respect to the development of plastic strain. A primary focus was to evaluate the influence of material constitutive models on predicted mechanical behaviors. Johnson-Cook (JC) constitutive material models were chosen to account for the contribution of strain, strain rate, and temperature on material responses. Three different modifications of the JC models were applied to assess the sensitivity of rate hardening and temperature-dependent strain hardening on mechanical prediction. Numerical results revealed that predicted stress and strain fields were highly sensitive to the rate-dependent term but minimally affected by the temperature-dependent strain hardening. The maximum differences in average stresses and plastic strains were 23\% and 13\% when comparing results between the JC model and the JC model without the strain rate hardening, respectively. Moreover, it was found that strain rate hardening mostly occurred at intermediate temperatures, {$\approx$}1000 \textendash{} 1600 K, during the continuous cooling phase. Finally, the present study emphasizes the importance of material constitutive models on mechanical behavior in laser powder bed fusion as well as providing insight for further exploration of process parameter optimization.},
  keywords = {additive manufacturing,Johnson-Cook plasticity model,laser powder bed fusion,Material constitutive models,Residual stress,Thermomechanical modeling,Ti-6Al-4V alloy}
}

@article{promoppatumPhysicsbasedPhenomenologicalPlasticity2021,
  title = {Physics-Based and Phenomenological Plasticity Models for Thermomechanical Simulation in Laser Powder Bed Fusion Additive Manufacturing: {{A}} Comprehensive Numerical Comparison},
  shorttitle = {Physics-Based and Phenomenological Plasticity Models for Thermomechanical Simulation in Laser Powder Bed Fusion Additive Manufacturing},
  author = {Promoppatum, Patcharapit and Rollett, Anthony D.},
  year = {2021},
  month = mar,
  journal = {Materials \& Design},
  pages = {109658},
  issn = {0264-1275},
  doi = {10.1016/j.matdes.2021.109658},
  abstract = {The present study investigated the sensitivity of material constitutive models on thermomechanical responses in laser powder bed fusion additive manufacturing of Ti-6Al-4\,V. Uniform scan strategies with scan lengths of 0.5, 1, and 2\,mm were applied so that wide ranges of thermal histories could be generated. The Johnson-Cook (JC) and Mechanical Threshold Stress (MTS) material plasticity models were chosen to capture the influence of strain, strain rate, and temperature. The JC model is a phenomenological model which is known for its easy implementation and excellent agreement with material testing results. On the other hand, the MTS model is a more complex physics-based internal state variable plasticity model that is expected to provide more accurate estimation, particularly for cases involving changes in strain rate and temperature. Numerical results revealed that both JC and MTS models provided a similar stress evolution, however, the plastic strain evolution was more realistic using the MTS model. Moreover, it was found that the maximum strain and the strain rate in the LPBF process are high compared to typical quasi-static testing, i.e., \textasciitilde 2\% and\,\textasciitilde\,4\,s-1, respectively. Accordingly, the material models should be calibrated with data obtained under similar deformation conditions. The choice of scan length also strongly affects in-plane stress anisotropy. Ultimately, we show both qualitatively and quantitatively the dependency of mechanical behavior prediction in LPBF on the choice of material models.},
  langid = {english},
  keywords = {t s},
  file = {/Users/zimbropa/Documents/Literaturverwaltung/storage/Q2CW4U7H/Promoppatum und Rollett - 2021 - Physics-based and phenomenological plasticity mode.pdf;/Users/zimbropa/Documents/Literaturverwaltung/storage/6VZHFSXG/S0264127521002112.html}
}

