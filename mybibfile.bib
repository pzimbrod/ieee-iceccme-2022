@article{liRevealingTransientPowderGas2020,
  title = {Revealing {{Transient Powder-Gas Interaction}} in {{Laser Powder Bed Fusion Process}} through {{Multi-physics Modeling}} and {{High-speed Synchrotron X-ray Imaging}}},
