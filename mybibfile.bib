@article{babacanLaserPowderBed2021,
  title = {Laser Powder Bed Fusion of a Superelastic {{Cu-Al-Mn}} Shape Memory Alloy},
  author = {Babacan, N. and Pauly, S. and Gustmann, T.},
  year = {2021},
  month = mar,
  journal = {Materials \& Design},
  pages = {109625},
  issn = {0264-1275},
  doi = {10.1016/j.matdes.2021.109625},
  abstract = {Dense and crack-free specimens of the shape memory alloy Cu71.6Al17Mn11.4 (at.\%) were produced via laser powder bed fusion across a wide range of process parameters. The microstructure, viz. grain size, can be directly tailored within the process and with it the transformation temperatures (TTs) shifted to higher values by raising the energy input. The microstructure, and the superelastic behavior of additively manufactured samples were assessed by a detailed comparison with induction melted material. The precipitation of the {$\alpha$} phase, which inhibit the martensitic transformation, were not observed in the additively manufactured samples owing to the high intrinsic cooling rates during the fabrication process. Fine columnar grains with a strong [001]-texture along the building direction lead to an enhanced yield strength compared to the coarse-grained cast samples. A maximum recoverable strain of 2.86\% was observed after 5\% compressive loading. The first results of our approach imply that laser powder bed fusion is a promising technique to directly produce individually designed Cu-Al-Mn shape memory parts with a pronounced superelasticity at room temperature.},
  langid = {english},
  keywords = {Additive manufacturing,Cu-Al-Mn,Laser powder bed fusion,Martensitic transformation,Shape memory alloy,Superelasticity},
  file = {/Users/zimbropa/Documents/Literaturverwaltung/storage/TTXV76AV/Babacan et al. - 2021 - Laser powder bed fusion of a superelastic Cu-Al-Mn.pdf;/Users/zimbropa/Documents/Literaturverwaltung/storage/FR8Z7MFY/S0264127521001787.html}
}

@article{babuskaAdditiveManufacturingDesign2020,
  title = {An Additive Manufacturing Design Approach to Achieving High Strength and Ductility in Traditionally Brittle Alloys via Laser Powder Bed Fusion},
