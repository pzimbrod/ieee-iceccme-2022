@article{gabrysiakDevelopmentCharacterizationMetastable2021,
  title = {Development and {{Characterization}} of a {{Metastable Al-Mn-Ce Alloy}} Produced by {{Laser Powder Bed Fusion}}},
  author = {Gabrysiak, Katharina and Gustmann, Tobias and Freudenberger, Jens and Neufeld, Kai and Giebeler, Lars and Leyens, Christoph and K{\"u}hn, Uta},
