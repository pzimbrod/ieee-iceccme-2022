  title = {In Situ {{X-ray}} Quantification of Melt Pool Behaviour during Directed Energy Deposition Additive Manufacturing of Stainless Steel},
  author = {Chen, Yunhui and Clark, Samuel J. and Huang, Yuze and Sinclair, Lorna and Lun Alex Leung, Chu and Marussi, Sebastian and Connolley, Thomas and Magdysyuk, Oxana V. and Atwood, Robert C. and Baxter, Gavin J. and Jones, Martyn A. and Todd, Iain and Lee, Peter D.},
