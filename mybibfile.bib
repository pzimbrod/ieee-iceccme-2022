@article{stavropoulosDevelopingFrameworkUsing2022,
  title = {Developing a {{Framework}} for {{Using Molecular Dynamics}} in {{Additive Manufacturing Process Modelling}}},
  author = {Stavropoulos, Panagiotis and Panagiotopoulou, Vasiliki Christina},
  year = {2022},
  month = mar,
  journal = {Modelling},
  volume = {3},
  number = {1},
  pages = {189--200},
