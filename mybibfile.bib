@article{niuAdditiveManufacturingTRIPassisted2021,
  title = {Additive Manufacturing of {{TRIP-assisted}} Dual-Phases {{Fe50Mn30Co10Cr10}} High-Entropy Alloy: {{Microstructure}} Evolution, Mechanical Properties and Deformation Mechanisms},
