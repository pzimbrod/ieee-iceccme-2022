}

@article{straumalGrainBoundaryWetting2021,
  title = {Grain {{Boundary Wetting}} by a {{Second Solid Phase}} in the {{High Entropy Alloys}}: {{A Review}}},
  shorttitle = {Grain {{Boundary Wetting}} by a {{Second Solid Phase}} in the {{High Entropy Alloys}}},
  author = {Straumal, Boris B. and Korneva, Anna and Lopez, Gabriel A. and Kuzmin, Alexei and Rabkin, Eugen and Gerstein, Gregory and Straumal, Alexander B. and Gornakova, Alena S.},
  year = {2021},
  month = jan,
  journal = {Materials},
  volume = {14},
  number = {24},
  pages = {7506},
  publisher = {{Multidisciplinary Digital Publishing Institute}},
  doi = {10.3390/ma14247506},
  abstract = {In this review, the phenomenon of grain boundary (GB) wetting by the second solid phase is analyzed for the high entropy alloys (HEAs). Similar to the GB wetting by the liquid phase, the GB wetting by the second solid phase can be incomplete (partial) or complete. In the former case, the second solid phase forms in the GB of a matrix, the chain of (usually lenticular) precipitates with a certain non-zero contact angle. In the latter case, it forms in the GB continuous layers between matrix grains which completely separate the matrix crystallites. The GB wetting by the second solid phase can be observed in HEAs produced by all solidification-based technologies. The particle chains or continuous layers of a second solid phase form in GBs also without the mediation of a liquid phase, for example by solid-phase sintering or coatings deposition. To describe the GB wetting by the second solid phase, the new GB tie-lines should be considered in the two- or multiphase areas in the multicomponent phase diagrams for HEAs. The GB wetting by the second solid phase can be used to improve the properties of HEAs by applying the so-called grain boundary engineering methods.},
  copyright = {http://creativecommons.org/licenses/by/3.0/},
  langid = {english},
  keywords = {grain boundary wetting,high entropy alloys,phase diagrams,phase transitions,precipitation},
  file = {/Users/zimbropa/Documents/Literaturverwaltung/storage/DKDWU949/Straumal et al. - 2021 - Grain Boundary Wetting by a Second Solid Phase in .pdf}
}

