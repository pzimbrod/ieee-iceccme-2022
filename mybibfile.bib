@article{suiAchievingGrainRefinement2021,
  title = {Achieving Grain Refinement and Ultrahigh Yield Strength in Laser Aided Additive Manufacturing of {{Ti}}-{{6Al}}-{{4V}} Alloy by Trace {{Ni}} Addition},
