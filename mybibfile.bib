@article{welkUseAlloyingEffect2021,
  title = {Use of {{Alloying}} to {{Effect}} an {{Equiaxed Microstructure}} in {{Additive Manufacturing}} and {{Subsequent Heat Treatment}} of {{High-Strength Titanium Alloys}}},
  author = {Welk, Brian A. and Taylor, Nevin and Kloenne, Zachary and Chaput, Kevin J. and Fox, Stephen and Fraser, Hamish L.},
