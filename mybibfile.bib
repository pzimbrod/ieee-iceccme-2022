  shorttitle = {Reduced Order Modeling of Selective Laser Melting},
  author = {Ghnatios, Chady and Rai, Khalil El and Hascoet, Nicolas and Pires, Pierre-Adrien and Duval, Jean-Louis and Lambarri, Jon and Hascoet, Jean-Yves and Chinesta, Francisco},
