@article{razumovStructurePropertiesTi2022,
  title = {Structure and {{Properties}} of {{Ti}}\textendash{{Zr}}\textendash{{Ni}}\textendash{{Co}}\textendash{{Cu High-Entropy Alloy After Powder Bed Fusion}} of {{Powders Produced}} by {{Mechanical Alloying}} and {{Plasma Spheroidization}}},
  author = {Razumov, N. G. and Makhmutov, T. Yu. and Kim, A. and Goncharov, I. S. and Ozerskoi, N. E. and Silin, A. O. and Borisov, E. V. and Starikov, K. A. and Mazeeva, A. K. and Popovich, A. A.},
