  title = {Nanoparticle-Mediated Ultra Grain Refinement and Reinforcement in Additively Manufactured Titanium Alloys},
  author = {Chao, Qi and Mateti, Srikanth and Annasamy, Murugesan and Imran, Mohammad and Joseph, Jithin and Cai, Qiran and Li, Lu Hua and Cizek, Pavel and Hodgson, Peter D. and Chen, Ying and Fabijanic, Daniel and Xu, Wei},
