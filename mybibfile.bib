@article{avateffazeliCorrelationTensileProperties2022,
  title = {Correlation between Tensile Properties, Microstructure, and Processing Routes of an {{Al}}\textendash{{Cu}}\textendash{{Mg}}\textendash{{Ag}}\textendash{{TiB2}} ({{A205}}) Alloy: {{Additive}} Manufacturing and Casting},
  shorttitle = {Correlation between Tensile Properties, Microstructure, and Processing Routes of an {{Al}}\textendash{{Cu}}\textendash{{Mg}}\textendash{{Ag}}\textendash{{TiB2}} ({{A205}}) Alloy},
  author = {Avateffazeli, M. and Carrion, P. E. and {Shachi-Amirkhiz}, B. and Pirgazi, H. and Mohammadi, M. and Shamsaei, N. and Haghshenas, M.},
  year = {2022},
  month = apr,
  journal = {Materials Science and Engineering: A},
  volume = {841},
  pages = {142989},
  issn = {0921-5093},
  doi = {10.1016/j.msea.2022.142989},
  abstract = {This paper aims at assessing microstructure/processing route/mechanical property correlation of a high-strength TiB2/Al\textendash Cu\textendash Mg\textendash Ag aluminum alloy, namely A205 alloy, additively manufactured via laser powder bed fusion (LPBF) versus the cast counter material. To this end, high magnification advanced microstructural characterization in connection with the tensile flow and strain hardening (i.e., work hardening) behavior of the materials were studied. Ambient temperature uniaxial tensile tests, based on ASTM E8/E8M, were conducted on the LPBF and cast as well as post-heat treated (T7: overaged and stabilized) materials were performed systematically. Results show a pronounced discontinuous yielding for the LPBF material along with extended ductility. The tensile curve of the LPBF T7 heat-treated material showed some plastic instabilities (Portevin\textendash Le Chatelier effect) in the inelastic region. However, none of these phenomena were observed in the cast material, while ductility is limited. These responses were attributed to the very high cooling/solidification rates experienced by the LPBF materials, which result in a very fine equiaxed and supersaturated aluminum grain structure as compared with the coarse-grained structure of the cast counter material.},
  langid = {english},
  keywords = {A205,Additive manufacturing,Al–Cu–Mg–Ag–TiB,Discontinuous yielding,Laser powder bed fusion,PLC effect,Yield point phenomenon},
  file = {/Users/zimbropa/Documents/Literaturverwaltung/storage/W2BBBY77/S0921509322003963.html}
}

@article{aversaNewAluminumAlloys2019,
  title = {New {{Aluminum Alloys Specifically Designed}} for {{Laser Powder Bed Fusion}}: {{A Review}}},
  shorttitle = {New {{Aluminum Alloys Specifically Designed}} for {{Laser Powder Bed Fusion}}},
  author = {Aversa, Alberta and Marchese, Giulio and Saboori, Abdollah and Bassini, Emilio and Manfredi, Diego and Biamino, Sara and Ugues, Daniele and Fino, Paolo and Lombardi, Mariangela},
  year = {2019},
