  title = {{{GPU-Accelerated Discontinuous Galerkin Methods}}: 30x {{Speedup}} on 345  {{Billion Unknowns}}},
  author = {Kirby, Andrew C. and Mavriplis, Dimitri J.},
  year = {2020},
