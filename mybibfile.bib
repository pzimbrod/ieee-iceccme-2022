  author = {{Barriobero-Vila}, Pere and Artzt, Katia and Stark, Andreas and Schell, Norbert and Siggel, Martin and Gussone, Joachim and Kleinert, Jan and Kitsche, Wolfgang and Requena, Guillermo and Haubrich, Jan},
