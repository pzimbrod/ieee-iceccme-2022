@article{kimSelectiveLaserMelted2021,
  title = {Selective Laser Melted {{CrMnFeCoNi}} + 3~Wt\% {{Y2O3}} High-Entropy Alloy Matrix Nanocomposite: {{Fabrication}}, Microstructure and Nanoindentation Properties},
  shorttitle = {Selective Laser Melted {{CrMnFeCoNi}} + 3~Wt\% {{Y2O3}} High-Entropy Alloy Matrix Nanocomposite},
  author = {Kim, Young-Kyun and Ahn, Ji-Eun and Song, Yongwook and Choi, Hyunjoo and Yang, Sangsun and Lee, Kee-Ahn},
  year = {2021},
  month = nov,
  journal = {Intermetallics},
  volume = {138},
  pages = {107319},
  issn = {0966-9795},
  doi = {10.1016/j.intermet.2021.107319},
  abstract = {A Y2O3-reinforced equiatomic CrMnFeCoNi high-entropy alloy (HEA) matrix nanocomposite was fabricated by high-energy attrition milling and selective laser melting (SLM) additive manufacturing. The SLM-built HEA nanocomposite possessed heterogeneous grain structures and substructures decorated with a dislocation network and exhibited a high number density of nano-sized Y2O3. The SLM-built HEA~+~Y2O3 nanocomposite exhibited higher nanohardness (\textasciitilde 9.22~GPa) than other equiatomic CrMnFeCoNi HEAs produced by casting (\textasciitilde 4.13~GPa) and SLM (\textasciitilde 6.95~GPa). This suggested that the dispersion hardening by the Y2O3 nanoparticles enabled superior mechanical properties. This study, therefore, demonstrated that Y2O3 reinforcement can effectively improve the mechanical properties of SLM-built CrMnFeCoNi HEA matrix nanocomposites.},
  langid = {english},
  keywords = {High-entropy alloy,Microstructure,Nanocomposite,Nanoindentation,Selective laser melting,YO},
  file = {/Users/zimbropa/Documents/Literaturverwaltung/storage/C9X7TBS8/S0966979521002351.html}
}

@article{kimStabilizedSubGrain2022,
  title = {Stabilized Sub\textendash Grain and Nano Carbides\textendash Driven 1.2 {{GPa}} Grade Ultra\textendash Strong {{CrMnFeCoNi}} High\textendash Entropy Alloy Additively Manufactured by Laser Powder Bed Fusion},
  author = {Kim, Young-Kyun and Lee, Kee-Ahn},
  year = {2022},
  month = jan,
  journal = {Journal of Materials Science \& Technology},
  issn = {1005-0302},
  doi = {10.1016/j.jmst.2021.12.010},
  abstract = {High\textendash entropy alloys (HEAs) with interstitial atoms that are produced by additive manufacturing have gained intensive interest in the materials science community because of their suitability for constructing high-strength net-shape components. Here, a strategy to additionally enhance the strength of selective laser melted carbon-containing HEAs was investigated. The as\textendash built carbon-containing HEAs (Cx(Cr20Mn20Fe20Co20Ni20)100\textendash x (x\,=\,0.5 at.\%, 1.0 at.\%, and 1.5 at.\%)) contain supersaturated carbon, and the extent of supersaturation increases as the carbon content increases. When subjected to aging treatment at 650\textdegree C for 1~h, the microstructure of the three alloys did not change at the grain scale. However, the microstructure at the sub-grain scale changed markedly, and these changes influenced the tensile properties and deformation mechanism. In particular, the tensile strength of aged 1.5C\textendash HEA at 650\textdegree C was {$\sim$}1.2 GPa at room temperature, which is higher than those reported for CrMnFeCoNi HEAs. Furthermore, the main deformation mechanism changed from deformation twinning to dislocation-mediated slip, resulting in much higher strain hardening capacity after the aging treatment. This work led to the development of an alternative promising method that involves tailoring the microstructure, to enhance the mechanical properties of additively manufactured metallic materials that contain interstitial atoms.},
  langid = {english},
  keywords = {Aging,Carbon contents,Deformation mechanism,High-entropy alloy,Microstructure evolution,Selective laser melting,Tensile property},
  file = {/Users/zimbropa/Documents/Literaturverwaltung/storage/LLXE9GVE/S1005030222000822.html}
}

@article{kimSuperiorTemperatureDependentMechanical2020,
  title = {Superior {{Temperature-Dependent Mechanical Properties}} and {{Deformation Behavior}} of {{Equiatomic CoCrFeMnNi High-Entropy Alloy Additively Manufactured}} by {{Selective Laser Melting}}},
  author = {Kim, Young-Kyun and Yang, Sangsun and Lee, Kee-Ahn},
  year = {2020},
  month = jan,
  journal = {Scientific Reports},
  volume = {10},
  number = {1},
  pages = {8045},
  issn = {2045-2322},
  doi = {10.1038/s41598-020-65073-2},
  abstract = {The microstructure, temperature-dependent mechanical properties and deformation behaviors of equiatomic CoCrFeMnNi high-entropy alloy (HEA) additively manufactured by selective laser melting (SLM) were investigated. SLM-built HEA had a face-centered cubic (FCC) single-phase random solid solution. In addition, SLM-built HEA was composed of epitaxial growth grains, dislocation network and nano-sized oxides. Room- and high-temperature compression tests confirmed that SLM-built HEA has outstanding mechanical properties in all temperature ranges compared to equiatomic CoCrFeMnNi HEAs reported up to the present. The excellent mechanical properties of SLM-built HEA were achieved with fine grains, high dislocation density and fine precipitates at low temperatures (25\,\textdegree C to 600\,\textdegree C), and by high dislocation density and fine precipitates at high temperatures (700\,\textdegree C or higher). On the other hand, the deformation microstructure showed that slip and deformation twins are the main deformation mechanisms from 25\,\textdegree C to 600\,\textdegree C, and slip and partial recrystallization are the main deformation mechanisms above 700\,\textdegree C. Based on the above findings, this study also discusses correlations among the microstructure, superior mechanical properties and deformation mechanisms of SLM-built equiatomic CoCrFeMnNi HEA.},
  pmid = {32415215},
  file = {/Users/zimbropa/Documents/Literaturverwaltung/storage/SZAB9AHP/Kim et al. - 2020 - Superior Temperature-Dependent Mechanical Properti.pdf}
}

@article{kingLaserPowderBed2015,
  title = {Laser Powder Bed Fusion Additive Manufacturing of Metals; Physics, Computational, and Materials Challenges},
  author = {King, W. E. and Anderson, A. T. and Ferencz, R. M. and Hodge, N. E. and Kamath, C. and Khairallah, S. A. and Rubenchik, A. M.},
