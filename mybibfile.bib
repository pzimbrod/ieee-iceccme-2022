@article{pistorNovelMechanismGenerate2021,
  title = {A Novel Mechanism to Generate Metallic Single Crystals},
  author = {Pistor, J. and K{\"o}rner, C.},
  year = {2021},
  month = dec,
  journal = {Scientific Reports},
