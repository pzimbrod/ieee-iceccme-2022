@inproceedings{katz-demyanetzInsituAlloyingNovel2020,
  title = {In-Situ {{Alloying}} as a {{Novel Methodology}} in {{Additive Manufacturing}}},
  booktitle = {2020 {{IEEE}} 10th {{International Conference Nanomaterials}}: {{Applications Properties}} ({{NAP}})},
  author = {{Katz-Demyanetz}, A. and Koptyug, A. and Popov, V. V.},
  year = {2020},
  month = nov,
  pages = {02SAMA05-1-02SAMA05-4},
  doi = {10.1109/NAP51477.2020.9309652},
  abstract = {Additive Manufacturing (AM) of metallic alloys, ceramics and composites often requires a use of prealloyed powders as raw materials. Since synthesizing of such powders is complicated and expensive, an issue of in-situ processing becomes relevant and is increasingly studied nowadays. The main idea of in-situ alloying assisted processing, namely the alloying/reaction occurring between materials of different powders, can be implemented at three principally separate processing stages: pre-processing (raw material preparation), printing itself and post-processing (sintering/infiltration/heat-treatment). The presented brief review gives a necessary background for in-situ alloying at the mostly employed pre-process, process and post-process stages relevant for Powder Bed Fusion (PBF), Direct Energy Deposition (DED) and Binder Jet Printing (BJP) technologies. The presented survey discusses the involved physical processes with their relevance for each mentioned AM technologies and proposes a novel methodology for designing new materials.},
  keywords = {additive manufacturing,Alloying,Binder Jetting,diffusion,in situ alloying,infiltration,Metals,Microstructure,Powder Bed Fusion,Powders,pre-processing,Production,Raw materials,Three-dimensional printing},
  file = {/Users/zimbropa/Documents/Literaturverwaltung/storage/XNYKRWFF/Katz-Demyanetz et al. - 2020 - In-situ Alloying as a Novel Methodology in Additiv.pdf;/Users/zimbropa/Documents/Literaturverwaltung/storage/L4LZDN6Y/9309652.html}
