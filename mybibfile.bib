@article{siaoThermalAnalysisAnisotropic2021,
  title = {Thermal Analysis of Anisotropic Heat Conduction Model with Experimental Validation on Molten Pool during Selective Laser Melting},
  author = {Siao, Yong-Hao and Wen, Chang-Da},
