@article{tangAlloysByDesignApplicationNew2020,
  title = {Alloys-{{By-Design}}: {{Application}} to {{New Superalloys}} for {{Additive Manufacturing}}},
  author = {Tang, Yuanbo T. and Panwisawas, Chinnapat and Ghoussoub, Joseph N. and Gong, Yilun and Clark, John and N{\'e}meth, Andr{\'e} and McCartney, D. Graham and Reed, Roger C.},
