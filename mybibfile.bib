  title = {The Intelligent Recoater: A New Solution for in-Situ Monitoring of Geometric and Surface Defects in Powder Bed Fusion},
  shorttitle = {The Intelligent Recoater},
  author = {Bugatti, Matteo and Colosimo, Bianca Maria},
