@article{shenMultiTaskGaussianProcess2022,
  title = {Multi-{{Task Gaussian Process Upper Confidence Bound}} for {{Hyperparameter Tuning}} and Its {{Application}} for {{Simulation Studies}} of {{Additive Manufacturing}}},
  author = {Shen, Bo and Gnanasambandam, Raghav and Wang, Rongxuan and Kong, Zhenyu (James)},
  year = {2022},
  month = feb,
  journal = {IISE Transactions},
  volume = {0},
  number = {ja},
  pages = {1--30},
  publisher = {{Taylor \& Francis}},
  issn = {2472-5854},
  doi = {10.1080/24725854.2022.2039813},
  abstract = {In many scientific and engineering applications, Bayesian optimization (BO) is a powerful tool for hyperparameter tuning of a machine learning model, materials design and discovery, etc. Multi-task Bayesian optimization is a general method to efficiently optimize multiple different but correlated ``black-box'' functions. The objective of this work is to develop an algorithm for multi-task Bayesian optimization with automatic task selection so that only one task evaluation is needed per query round. Specifically, a new algorithm, namely, multi-task Gaussian process upper confidence bound (MT-GPUCB), is proposed to achieve this objective. The MT-GPUCB is a two-step algorithm, where the first step chooses which query point to evaluate, and the second step automatically selects the most informative task to evaluate. Under the bandit setting, a theoretical analysis is provided to show that our proposed MT-GPUCB is no-regret under some mild conditions. Our proposed algorithm is verified experimentally on a range of synthetic functions. In addition, our algorithm is applied to Additive Manufacturing simulation software, namely, Flow-3D Weld, to determine material property values, ensuring the quality of simulation output. The results clearly show the advantages of our query strategy for both design point and task.},
  keywords = {Automatic Task Selection,Hyperparameter Tuning,Multi-task Gaussian Process Upper Confidence Bound,No-Regret},
  annotation = {\_eprint: https://www.tandfonline.com/doi/pdf/10.1080/24725854.2022.2039813},
  file = {/Users/zimbropa/Documents/Literaturverwaltung/storage/XVUUI9YY/24725854.2022.html}
}

