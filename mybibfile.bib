@article{hosseinzadehNumericalApproachModel2022,
  title = {A {{Numerical Approach}} to {{Model Microstructure Evolution}} for {{NiTi Shape Memory Alloy}} in {{Laser Powder Bed Fusion Additive Manufacturing}}},
