  title = {In {{Situ Measurements}} of {{Melt-Pool Length}} and {{Cooling Rate During 3D Builds}} of the {{Metal AM-Bench Artifacts}}},
  author = {Heigel, Jarred C. and Lane, Brandon M. and Levine, Lyle E.},
