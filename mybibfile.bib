@article{ferrettoLaserPowderBed2021,
  title = {Laser Powder Bed Fusion of a {{Fe}}\textendash{{Mn}}\textendash{{Si}} Shape Memory Alloy},
  author = {Ferretto, I. and Kim, D. and Della Ventura, N. M. and Shahverdi, M. and Lee, W. and Leinenbach, C.},
  year = {2021},
