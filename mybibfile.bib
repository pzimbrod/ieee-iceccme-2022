@article{leePrecipitationKineticsSecondary2022,
  title = {Precipitation Kinetics of Secondary Phases Induced by Heat Accumulation in the Deposit of {{Inconel}} 718},
  author = {Lee, Seung Hwan and Van, Donghyun and Jang, Junmyoung},
  year = {2022},
