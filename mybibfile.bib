  title = {Numerical Modeling of Oxide Particle Evolution during Additive Manufacturing},
  author = {Eo, Du-Rim and Chung, Seong-Gyu and Yang, JeongHo and Jung, In-Ho and Cho, Jung-Wook},
  year = {2022},
