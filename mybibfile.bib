@article{haferkampInfluenceParticleShape2021,
  title = {The {{Influence}} of {{Particle Shape}}, {{Powder Flowability}}, and {{Powder Layer Density}} on {{Part Density}} in {{Laser Powder Bed Fusion}}},
  author = {Haferkamp, Lukas and Haudenschild, Livia and Spierings, Adriaan and Wegener, Konrad and Riener, Kirstin and Ziegelmeier, Stefan and Leichtfried, Gerhard J.},
