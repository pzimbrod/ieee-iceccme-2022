  file = {/Users/zimbropa/Documents/Literaturverwaltung/storage/WWCHVCJ3/Haferkamp et al. - 2021 - The Influence of Particle Shape, Powder Flowabilit.pdf}
}

@article{hagerDevelopmentHighDensity2022,
  title = {Development of High Density Parts in the Low-Alloy, High-Performance Steel {{AF9628}} Using Laser Powder Bed Fusion},
  author = {Hager, E. M. and O'Hara, R. P. and Cobb, G. R. and Flater, P. J. and Payton, E. J. and Sinha, V. and Doane, B. M. and Kemnitz, R. A.},
  year = {2022},
  month = jan,
  journal = {Materials Science and Engineering: A},
  pages = {142656},
  issn = {0921-5093},
  doi = {10.1016/j.msea.2022.142656},
  abstract = {A process parameter development study was performed in order to determine the printability of a low-alloy, high-performance steel, AF9628, using laser powder bed fusion. A weld track study was performed using 40 distinct laser power and speed combinations in order to determine which combinations produced acceptable conduction welds that would yield high-quality parts. Ten combinations with three distinct hatch spacing values were selected to create cylindrical specimens for porosity studies. Eight out of the ten combinations resulted in parts that were\,{$>$}99.5\% dense. Two combinations that produced\,{$>$}99.9\% dense parts were used to produce tensile specimens. Tensile testing revealed that the ultimate tensile strength (UTS) for as-printed specimens manufactured using both of the processing conditions was significantly higher ({$>$}24\%) than the literature values for wrought AF9628. Heat-treating the specimens reduced their UTS values, but they still exceeded the literature value by 8\%. Hardness measurements indicate that the Vickers hardness is approximately 10\% lower for the as-printed specimens when compared to the literature value for wrought AF9628, while it is at least 6\% greater for the heat-treated specimens than the wrought AF9628. Electron backscatter diffraction results showed that the as-printed microstructure exhibited features typical of the martensitic transformation in quench-and-temper steels.},
  langid = {english},
  keywords = {Additive manufacturing,Fluence,Heat treatment,Laser powder bed fusion,Process parameter development,Steel},
  file = {/Users/zimbropa/Documents/Literaturverwaltung/storage/FLKYJIPZ/S0921509322000648.html}
}

@article{hagiharaControlAnisotropicCrystallographic2021,
