@article{vockPowdersPowderBed2019,
  title = {Powders for Powder Bed Fusion: A Review},
  shorttitle = {Powders for Powder Bed Fusion},
  author = {Vock, Silvia and Kl{\"o}den, Burghardt and Kirchner, Alexander and Wei{\ss}g{\"a}rber, Thomas and Kieback, Bernd},
