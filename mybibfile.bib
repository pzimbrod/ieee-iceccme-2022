@article{farzadfarImpactIN718Bimodal2020,
  title = {Impact of {{IN718}} Bimodal Powder Size Distribution on the Performance and Productivity of Laser Powder Bed Fusion Additive Manufacturing Process},
  author = {Farzadfar, Seyed Amir and Murtagh, Martin J. and Venugopal, Navin},
  year = {2020},
