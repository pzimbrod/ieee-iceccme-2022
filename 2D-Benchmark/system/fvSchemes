FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         Euler;
}

gradSchemes
{
    default         cellLimited Gauss linear 1;
    grad(U)         cellLimited Gauss linear 1;
}

divSchemes
{
    div(rhoPhi,U)           Gauss WENOUpwindFit 3 1;

    "div\(alphaPhi.*,Yi\)"  Gauss WENOUpwindFit 3 1;

    "div\(phi,alpha.*\)"    Gauss WENOUpwindFit 3 1;
    "div\(phir,alpha.*\)"   Gauss WENOUpwindFit 3 1;

    DDt(phi,alpha)          Gauss WENOUpwindFit 3 1;

    div(((rho*nuEff)*dev2(T(grad(U))))) Gauss linear;

    div(phi,T) Gauss linear;

    div(HbyA)               Gauss WENOUpwindFit 3 1;

}

laplacianSchemes
{
    //default             Gauss linear orthogonal;
    default               Gauss linear limited 1;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         orthogonal;
    //default               Gauss linear limited 1;
}

fluxRequired
{
    default         no;
    p_rgh             ;
    "alpha.*"         ;
}


// ************************************************************************* //
